<Window x:Class="JXHighWay.WatchHouse.WFPClient.LEDSend"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JXHighWay.WatchHouse.WFPClient"
        mc:Ignorable="d"
        Title="LEDSend" Height="500" Width="500" AllowsTransparency="True" WindowStyle="None">
    <Grid  Background="#e4e4e4">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="177*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions >
            <RowDefinition Height="4*"   />
            <RowDefinition Height="105*" />
            <RowDefinition Height="16*"  />
        </Grid.RowDefinitions>
        <Image HorizontalAlignment="Left" Grid.Column="0" Grid.Row="0" Height="20" VerticalAlignment="Top" Width="500" Source="Images/LED/Tou.jpg" Grid.RowSpan="2"/>
        <Image HorizontalAlignment="Left" x:Name="Image_Close" Height="13" VerticalAlignment="Top" Width="13" Source="Images/LED/GuanBi.jpg" MouseLeftButtonDown="Image_Close_MouseLeftButtonDown" Cursor="Hand" Margin="486,1,0,0"/>
        <GroupBox Header="GroupBox" HorizontalAlignment="Left" Height="155" VerticalAlignment="Top" Width="499" Grid.Row="1"/>
    </Grid>
</Window>
