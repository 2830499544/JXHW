<Window x:Class="JXHighWay.WatchHouse.WFPClient.LEDText"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JXHighWay.WatchHouse.WFPClient"
        mc:Ignorable="d"
        Title="LEDText" Height="300" Width="500">
    <Grid Background="#245d7c">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="177*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions >
            <RowDefinition Height="44*"  />
            <RowDefinition Height="47*" />
            <RowDefinition Height="179*" />
        </Grid.RowDefinitions>
        <RichTextBox HorizontalAlignment="Left" Height="179" Grid.RowSpan="2" VerticalAlignment="Top" Width="494" Margin="0,47,-0.4,0" Grid.Row="1">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
    </Grid>
</Window>
