<Window x:Class="JXHighWay.WatchHouse.WFPClient.LEDText"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JXHighWay.WatchHouse.WFPClient"
        xmlns:rtf="clr-namespace:RTFEditor;assembly=RTFEditor"
        mc:Ignorable="d"
        Title="LEDText" Height="600" Width="800" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="177*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions >
            <RowDefinition Height="31*"  />
            <RowDefinition Height="254*" />
        </Grid.RowDefinitions>
        <rtf:RTFBox Name="RTFBox1" Grid.Column="0" Grid.Row="1"></rtf:RTFBox>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,19,0,0" Click="button_Click"/>
    </Grid>
</Window>
