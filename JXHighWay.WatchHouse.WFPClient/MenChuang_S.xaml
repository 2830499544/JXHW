                <Label x:Name="label_QFMD_Guan" Content="关×" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="9,17,0,0" FontSize="16" Foreground="#FFF0190F"/>
                <Label x:Name="label_QFMD_Kai" Content="√开" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="139,17,0,0" FontSize="16" Foreground="#FF079E23" IsEnabled="False" />
