        </Image>
        <Image HorizontalAlignment="Left" Height="43" Margin="671,107,0,0" Grid.Row="3" VerticalAlignment="Top" Width="72" >
            <Image.Style>
                <Style TargetType="{x:Type Image}">
                    <Setter Property="Source" Value="Images/KongTiao.jpg"/>
