            <Label x:Name="Label_Name_WH5" Content="5号岗亭" HorizontalAlignment="Center" Margin="35,146,39,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_JobNo_5" Content="工号:XXXXXX" HorizontalAlignment="Left" Margin="34,168,0,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_Name_WH6" Content="6号岗亭" HorizontalAlignment="Center" Margin="39,146,34,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="1" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_JobNo_6" Content="工号:XXXXXX" HorizontalAlignment="Left" Margin="38,168,0,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="1" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_Name_WH7" Content="7号岗亭" HorizontalAlignment="Center" Margin="36,146,38,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="2" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_JobNo_7" Content="工号:XXXXXX" HorizontalAlignment="Left" Margin="37,168,0,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="2" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_Name_WH8" Content="8号岗亭" HorizontalAlignment="Center" Margin="33,146,40,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="3" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_JobNo_8" Content="工号:XXXXXX" HorizontalAlignment="Left" Margin="35,168,0,0" Grid.Row="2" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="3" HorizontalContentAlignment="Center"/>
        </Grid>

    </Grid>
</Page>
