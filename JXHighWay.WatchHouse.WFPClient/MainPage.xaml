<Page x:Class="JXHighWay.WatchHouse.WFPClient.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:JXHighWay.WatchHouse.WFPClient"
      mc:Ignorable="d" 
      d:DesignHeight="460" d:DesignWidth="1024"
      Title="MainPage" Loaded="Page_Loaded">

    <Grid  Background="#245d7c">
        <Grid Height="460" VerticalAlignment="Top" Margin="65,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="90*"/>
                <ColumnDefinition Width="90*"/>
                <ColumnDefinition Width="90*"/>
                <ColumnDefinition Width="90*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="56"   />
                <RowDefinition Height="205*"  />
                <RowDefinition Height="199*" />

            </Grid.RowDefinitions>
            <Label x:Name="Label_Title" Content="XXXXXX收费站" HorizontalAlignment="Center" Margin="106,3,101.5,7" Grid.ColumnSpan="2" Width="240" FontSize="36" Grid.Column="1" Foreground="#FFF9F7F7" HorizontalContentAlignment="Center" FontFamily="STHupo"/>
            <Image x:Name="Image_WH1" HorizontalAlignment="Left" Height="148" Margin="36,4,0,0" Grid.Row="1" VerticalAlignment="Top" Width="148" Cursor="Hand">
                <Image.Style>
                    <Style TargetType="{x:Type Image}">
                        <Setter Property="Source" Value="Images/Main/Chu.jpg"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver"  Value="True">
                                <Setter Property="Source" Value="Images/Main/Chu_A.jpg"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Image.Style>
            </Image>
            <Image x:Name="Image_WH2" HorizontalAlignment="Left" Height="148" Margin="37,4,0,0" Grid.Row="1" VerticalAlignment="Top" Width="148" Grid.Column="1" Cursor="Hand">
                <Image.Style>
                    <Style TargetType="{x:Type Image}">
                        <Setter Property="Source" Value="Images/Main/Ru.jpg"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver"  Value="True">
                                <Setter Property="Source" Value="Images/Main/Ru_A.jpg"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Image.Style>
            </Image>
            <Image x:Name="Image_WH3" HorizontalAlignment="Left" Height="148" Margin="36,4,0,0" Grid.Row="1" VerticalAlignment="Top" Width="148" Source="Images/GanTing.jpg" Grid.Column="2"/>
            <Image x:Name="Image_WH4" HorizontalAlignment="Left" Height="148" Margin="34,4,0,0" Grid.Row="1" VerticalAlignment="Top" Width="148" Source="Images/GanTing.jpg" Grid.Column="3"/>
            <Image x:Name="Image_WH5" HorizontalAlignment="Left" Height="148" Margin="36,7,0,0" Grid.Row="2" VerticalAlignment="Top" Width="148" Source="Images/GanTing.jpg"/>
            <Image x:Name="Image_WH6" HorizontalAlignment="Left" Height="148" Margin="37,7,0,0" Grid.Row="2" VerticalAlignment="Top" Width="148" Source="Images/GanTing_Down.jpg" Grid.Column="1"/>
            <Image x:Name="Image_WH7" HorizontalAlignment="Left" Height="148" Margin="36,7,0,0" Grid.Row="2" VerticalAlignment="Top" Width="148" Source="Images/GanTing_Down.jpg" Grid.Column="2"/>
            <Image x:Name="Image_WH8" HorizontalAlignment="Left" Height="148" Margin="34,7,0,0" Grid.Row="2" VerticalAlignment="Top" Width="148" Source="Images/GanTing_Down.jpg" Grid.Column="3"/>
            <Label x:Name="Label_Name_WH1" Content="1号岗亭" HorizontalAlignment="Center" Margin="35,151,39,0" Grid.Row="1" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_JobNo_1" Content="工号:XXXXXX" HorizontalAlignment="Left" Margin="38,174,0,0" Grid.Row="1" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_Name_WH2" Content="2号岗亭" HorizontalAlignment="Center" Margin="36,152,37,0" Grid.Row="1" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="1" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_JobNo_2" Content="工号:XXXXXX" HorizontalAlignment="Left" Margin="41,174,0,0" Grid.Row="1" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="1" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_Name_WH3" Content="3号岗亭" HorizontalAlignment="Center" Margin="37,152,37,0" Grid.Row="1" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="2" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_JobNo_3" Content="工号:XXXXXX" HorizontalAlignment="Left" Margin="39,174,0,0" Grid.Row="1" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="2" HorizontalContentAlignment="Center"/>
            <Label x:Name="Label_Name_WH4" Content="4号岗亭" HorizontalAlignment="Center" Margin="34,152,39,0" Grid.Row="1" VerticalAlignment="Top" FontSize="16" Foreground="#FFF7EEEE" Height="31" Width="150" Grid.Column="3" HorizontalContentAlignment="Center"/>
